package kodlama.io.gun2odev.business.responses;

public class CreateProgramingLanguageRequest {
	
	private String languageName;
	private String technology_Name;
	
	
	public CreateProgramingLanguageRequest() {
		
	}
	
	
	public CreateProgramingLanguageRequest(String languageName, String technology_Name) {
		this.languageName = languageName;
		this.technology_Name = technology_Name;
	}
	
	
	public String getLanguageName() {
		return languageName;
	}
	public void setLanguageName(String languageName) {
		this.languageName = languageName;
	}
	public String getTechnology_Name() {
		return technology_Name;
	}
	public void setTechnology_Name(String technology_Name) {
		this.technology_Name = technology_Name;
	}
	
	
	

}
